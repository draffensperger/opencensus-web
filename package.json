    "access": "public"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "gts": "^0.5.4",
    "jasmine-core": "^3.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-commonjs": "^1.0.0",
    "karma-jasmine": "^1.1.2",
    "karma-typescript": "^3.0.13",
    "typescript": "~2.7.2",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "swagger-js-codegen": "^1.12.0"
  }
